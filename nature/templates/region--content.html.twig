{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{% if content %}
  <div{{ attributes }}>
    {{ content }}
  </div>
            <div class="col-1-od-2">
                <div class="o-nama-slike">
                    <img  src="/themes/nature/img/landscape.jpg" alt="Sunshine" class="slika-1">  
                    <img  src="/themes/nature/img/secret-garden-tour.jpg" alt="Vodopad"  class="slika-2">  
                    <img  src="/themes/nature/img/tourimg.jpg" alt="Planina"  class="slika-3">  
                </div>
              </div>
{% endif %}
